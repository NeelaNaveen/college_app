<h1>Listing registration_rooms</h1>

<table>
  <thead>
    <tr>
      <th>Students</th>
      <th>Courses</th>
      <th>Semester</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @registration_rooms.each do |registration_room| %>
      <tr>
        <td><%= registration_room.students %></td>
        <td><%= registration_room.courses %></td>
        <td><%= registration_room.semester %></td>
        <td><%= link_to 'Show', registration_room %></td>
        <td><%= link_to 'Edit', edit_registration_room_path(registration_room) %></td>
        <td><%= link_to 'Destroy', registration_room, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Registration room', new_registration_room_path %>
